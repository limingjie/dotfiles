// Place your settings in this file to overwrite the default settings
{
    // Font
    "editor.fontFamily": "'Iosevka Light', Consolas, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,

    // Editor
    "editor.rulers": [80, 100, 120, 129, 137, 144, 150, 155, 159, 162, 164, 165],
    "editor.cursorBlinking": "phase",
    "editor.renderWhitespace": "boundary",
    "editor.renderIndentGuides": true,
    "editor.renderControlCharacters": true,
    "editor.mouseWheelZoom": true,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "editor.wordWrapColumn": 100,

    // Terminal
    "terminal.integrated.fontFamily": "'Iosevka Light', Consolas, 'Courier New', monospace",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.cursorBlinking": true,

    // Extensions
    "extensions.autoUpdate": true,

    // File
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.eol": "\n",
    "files.autoSave": "off",
    "files.associations": {"Makefile.*": "makefile"},

    // Git
    "git.path": "C:/Program Files/Git/cmd/git.exe",

    // clang
    "C_Cpp.clang_format_path": "C:/Program Files/LLVM/bin/clang-format.exe",
    "clang.executable": "C:/Program Files/LLVM/bin/clang.exe",
    "window.zoomLevel": 0,

    // golang
    "go.goroot": "C:/Go",
    "go.gopath": "C:/GoPath",
    "go.formatOnSave": true,

    // Trailing Spaces
    "trailing-spaces.highlightCurrentLine": false,

    // MISC
    "vsicons.dontShowNewVersionMessage": true,
    "git.confirmSync": false,
    "sync.gist": "784bf8de957289c43ea06893ed2ef700",
    "sync.lastUpload": "2017-06-28T05:21:31.952Z",
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.lastDownload": "1970-01-01T00:00:00.000Z",
    "sync.version": 262,
    "sync.showSummary": true,
    "sync.forceDownload": false,
    "sync.anonymousGist": false,
    "workbench.colorTheme": "Monokai",
    "sync.host": "",
    "sync.pathPrefix": "",
    "sync.quietSync": false,
    "workbench.iconTheme": "material-icon-theme",
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ]
}
